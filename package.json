    "nodemon": "^2.0.0",
    "nunjucks": "^3.2.0",
    "puppeteer": "^2.0.0",
    "sass": "^1.23.7",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.9",
    "@types/nunjucks": "^3.1.3",
    "@types/puppeteer": "^2.0.0",
    "@types/sass": "^1.16.0",
    "tsc-watch": "^4.0.0"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
