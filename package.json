{
  "name": "@javfres/javicv",
  "version": "2.1.0",
  "description": "My CV",
  "main": "build/index.js",
  "scripts": {
    "tsc": "tsc",
    "prepare": "tsc",
    "tsc-watch": "tsc -w",
    "cv": "node build/cmd.js",
    "dev": "concurrently -k -n \"TS,CV\" -c \"yellow.bold,cyan.bold\" \"npm run tsc-watch\" \"npm run dev-server\"",
    "dev-server": "nodemon --delay 2 --ext js,tsbuildinfo --watch build --exec \"node build/cmd.js --debug\""
  },
  "keywords": [
    "CV",
    "Resume"
  ],
  "author": "javfres",
  "license": "ISC",
  "dependencies": {
    "dist-exiftool": "^10.53.0",
    "express": "^4.17.3",
    "node-exiftool": "^2.3.0",
    "nunjucks": "^3.2.3",
    "puppeteer": "^13.4.1",
    "sass": "^1.49.9",
    "shelljs": "^0.8.5",
    "yargs": "^17.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16",
    "@types/nunjucks": "^3.2.1",
    "@types/puppeteer": "^13.4.1",
    "@types/sass": "^1.43.1",
    "@types/yargs": "^17.3",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  }
}
