{
  "name": "javicv",
  "version": "2.0.0",
  "description": "My CV",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "tsc-watch": "tsc -w",
    "build": "tsc && npm run pdf",
    "pdf": "node build/index.js",
    "pdf-watch": "nodemon --ext js,scss,njk --watch src --watch build --exec \"npm run pdf\"",
    "pdf-watch-delay-init": "sleep 10 && npm run pdf-watch",
    "dev": "concurrently -k -n \"TypeScript,Node PDF\" -c \"yellow.bold,cyan.bold\" \"npm run tsc-watch\" \"npm run pdf-watch-delay-init\""
  },
  "keywords": [
    "CV",
    "Resume"
  ],
  "author": "javfres",
  "license": "ISC",
  "dependencies": {
    "dist-exiftool": "^10.53.0",
    "express": "^4.17.1",
    "node-exiftool": "^2.3.0",
    "nunjucks": "^3.2.0",
    "puppeteer": "^2.0.0",
    "sass": "^1.23.7",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "tsc-watch": "^4.0.0",
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.0",
    "@types/node": "^12.12.9",
    "@types/nunjucks": "^3.1.3",
    "@types/puppeteer": "^2.0.0",
    "@types/sass": "^1.16.0",
    "@types/express": "^4.17.2",
    "typescript": "^3.7.2"
  }
}
